import sys

def factors(n):
    i = 2
    while i * i <= n:
        if n % i:
            i += 1
        else:
            return (i, n // i)
    return (n, 1)

with open(sys.argv[1], 'r') as f:
    for line in f:
        n = int(line.strip())
        p, q = factors(n)
        print(f"{n}={p}*{q}")

